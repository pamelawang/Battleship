public class GridButton extends JButton {
  
  private int xCoordinate;
  private int yCoordinate;
  
  public Gridbutton (int x, int y) {
    xCoordinate = x;
    yCoordiante = y;
  }
  
  public int getXCoord () {
    return xCoordinate;
  }
  
  public int getYCoord () {
    return yCoordinate;
  }  
  
}